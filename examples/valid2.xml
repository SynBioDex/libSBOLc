<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
   xmlns:so="http://purl.obolibrary.org/obo/" xmlns:s="http://sbols.org/v1#" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://sbols.org/v1# ../schema/sbol.xsd http://www.w3.org/1999/02/22-rdf-syntax-ns# ../schema/rdf.xsd">

   <s:DnaComponent rdf:about="http://example.com/dc1">
      <s:displayId>DC1</s:displayId>
      <s:name>DnaComponent1</s:name>
      <s:description>Various sequence annotations</s:description>
      <s:dnaSequence>
         <s:DnaSequence rdf:about="http://example.com/ds1">
            <s:nucleotides>tccctatcagtgat</s:nucleotides>
         </s:DnaSequence>
      </s:dnaSequence>
      <s:annotation>
         <s:SequenceAnnotation rdf:about="http://example.com/sa1">
            <s:subComponent>
               <s:DnaComponent rdf:about="http://example.com/dc2">
                  <s:displayId>DC2</s:displayId>
                  <s:name>DnaComponent2</s:name>
                  <s:description>Another DNA component</s:description>
               </s:DnaComponent>
            </s:subComponent>
         </s:SequenceAnnotation>
      </s:annotation>
      <s:annotation>
         <s:SequenceAnnotation rdf:about="http://example.com/sa2">
            <s:bioStart>1</s:bioStart>
            <s:bioEnd>54</s:bioEnd>
            <s:strand>+</s:strand>
            <s:subComponent>
               <s:DnaComponent rdf:about="http://example.com/dc3">
                  <s:displayId>DC3</s:displayId>
                  <s:name>DnaComponent3</s:name>
               </s:DnaComponent>
            </s:subComponent>
         </s:SequenceAnnotation>
      </s:annotation>
      <s:annotation>
         <s:SequenceAnnotation rdf:about="http://example.com/sa3">
            <s:precedes rdf:resource="http://example.com/sa2"/>
            <s:subComponent>
               <s:DnaComponent rdf:about="http://example.com/dc4">
                  <s:displayId>DC4</s:displayId>
                  <s:name>DnaComponent4</s:name>
               </s:DnaComponent>
            </s:subComponent>
         </s:SequenceAnnotation>
      </s:annotation>
   </s:DnaComponent>
</rdf:RDF>

